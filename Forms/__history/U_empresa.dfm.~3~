  inherited Q_padrao: TFDQuery
    SQL.Strings = (
      'SELECT '
      '  id_empresa,'
      '  razao_social,'
      '  n_fantasia,'
      '  endereco,'
      '  numero,'
      '  bairro,'
      '  cidade,'
      '  uf,'
      '  cep,'
      '  telefone,'
      '  cnpj,'
      '  email,'
      '  logo,'
      '  cadastro'
      'FROM '
      '  EMPRESA;'
      '')
